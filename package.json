{
  "name": "site-franchini",
  "version": "1.0.0",
  "description": "Site web de Franchini - Concessionnaire Deutz-Fahr",
  "private": true,
  "scripts": {
    "dev": "bundle exec jekyll serve --watch --port 3000 --incremental",
    "build": "bundle exec jekyll build",
    "start": "bundle exec jekyll serve --port 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '**/*.js' --quiet",
    "format": "prettier --write '**/*.{js,json,md,yml}'",
    "analyze": "bundle-audit check --update",
    "vercel-build": "chmod +x build.sh && ./build.sh"
  },
  "engines": {
    "node": "18.x",
    "ruby": ">=3.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5"
  },
  "browserslist": [
    "defaults"
  ]
}
