{
  "name": "site-franchini",
  "version": "1.0.0",
  "description": "Site web pour la concession Franchini - Concessionnaire Deutz-Fahr",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "bundle exec jekyll build",
    "vercel-build": "bash vercel-build.sh",
    "start": "bundle exec jekyll serve --livereload --trace",
    "clean": "rm -rf _site .sass-cache .jekyll-cache .jekyll-metadata",
    "minify": "uglifyjs assets/js/main.js -c -m --comments -o assets/js/main.min.js",
    "lint:js": "eslint assets/js/ --fix",
    "lint:css": "stylelint 'assets/css/**/*.scss'",
    "lint": "npm-run-all --parallel lint:*",
    "test": "echo \"ExÃ©cution des tests...\" && npm run lint",
    "dev": "gulp watch"
  },
  "engines": {
    "node": ">= 16.20.0",
    "npm": ">= 8.19.0"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "uglify-js": "^3.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "del": "^7.0.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3"
  },
  "uglify-js": {
    "mangle": true,
    "compress": {
      "drop_console": false
    },
    "output": {
      "comments": "/^!/"
    },
    "warnings": true,
    "ie8": false,
    "keep_fnames": false,
    "toplevel": false,
    "safari10": false,
    "ecma": 2017
  },
  "browserslist": [
    "defaults"
  ],
  "vercel": {
    "outputDirectory": "_site"
  }
}
