{
  "name": "site-franchini",
  "version": "1.0.0",
  "description": "Site web de Franchini - Concessionnaire Deutz-Fahr",
  "private": true,
  "scripts": {
    "dev": "bundle exec jekyll serve --watch --port 3000 --incremental",
    "build": "bundle exec jekyll build",
    "start": "bundle exec jekyll serve --port 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '**/*.js' --quiet",
    "format": "prettier --write '**/*.{js,json,md,yml}'",
    "analyze": "bundle-audit check --update"
  },
  "engines": {
    "node": ">=16.0.0",
    "ruby": ">=3.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.x",
    "@vercel/static-build": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^8.x",
    "eslint-config-prettier": "^9.x",
    "prettier": "^3.x",
    "vercel": "^33.x"
  },
  "vercel": {
    "buildCommand": "npm install && gem install bundler && bundle install && bundle exec jekyll build",
    "outputDirectory": "_site"
  },
  "browserslist": [
    "defaults"
  ]
}
