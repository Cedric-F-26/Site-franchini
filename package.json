{
  "name": "site-franchini",
  "version": "1.0.0",
  "description": "Site web du concessionnaire Franchini",
  "private": true,
  "scripts": {
    "start": "bundle exec jekyll serve --livereload --incremental",
    "build": "bundle exec jekyll build",
    "build:vercel": "npm install --ignore-scripts && bundle install && npm run build",
    "serve": "bundle exec jekyll serve",
    "clean": "bundle exec jekyll clean",
    "test": "npm run lint:js && npm run lint:css",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint 'assets/js/**/*.js'",
    "lint:css": "stylelint 'assets/css/**/*.css'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,html,css,scjs}'",
    "prepare": "[ -d .git ] && husky install || echo 'Skipping husky install (not a git repository)'"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=8.0.0",
    "ruby": ">= 2.7.0 < 3.5.0"
  },
  "packageManager": "npm@8.19.4",
  "dependencies": {
    "@vercel/static-build": "^1.3.35"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  }
}