{
  "version": 2,
  "builds": [
    {
      "src": "Gemfile",
      "use": "@vercel/ruby",
      "config": {
        "distDir": "_site"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "build": {
    "env": {
      "JEKYLL_ENV": "production",
      "BUNDLE_WITHOUT": "development:test"
    }
  }
}
