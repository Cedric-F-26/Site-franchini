{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "build": {
    "env": {
      "JEKYLL_ENV": "production"
    },
    "buildCommand": "bundle install && bundle exec jekyll build --config _config.yml,_config_vercel.yml --trace",
    "outputDirectory": "_site",
    "installCommand": "gem install bundler"
  }
}
