{
  "version": 2,
  "functions": {
    "vercel-build.sh": {
      "memory": 3008,
      "maxDuration": 300,
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).(jpg|jpeg|png|gif|ico|css|js|svg|woff|woff2|ttf|eot|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_VERSION": "18.x",
    "RUBY_VERSION": "3.2.2",
    "BUNDLER_VERSION": "2.4.22",
    "NODE_ENV": "production",
    "JEKYLL_ENV": "production",
    "BUNDLE_PATH": "/var/task/vendor/bundle",
    "BUNDLE_GEMFILE": "./Gemfile",
    "BUNDLE_JOBS": "4",
    "BUNDLE_RETRY": "3"
  },
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "_site",
  "regions": ["cdg1"]
}