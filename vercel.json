{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "_site",
        "cleanUrls": true,
        "trailingSlash": false
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(?:css|js|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*\\.(?:css|js|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)/$",
      "dest": "/$1/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1.html"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "16.x"
  },
  "buildCommand": "bundle exec jekyll build",
  "outputDirectory": "_site"
}
